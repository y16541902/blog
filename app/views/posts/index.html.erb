<div class="container">
  <div class="row">
   <div class="col-md-12">
<p id="notice"><%= notice %></p>
<br>
<br>
<br>
<h3 class="text-center">İşyerimizi Değerlendirmek İstermisiniz ?</h3>
<h6 class="text-center"> (Beğeni veya Eleştirilerinizi Lütfen Bizler ile Paylaşınız)</h6>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Ad</th>
      <th>Soyad</th>
      <th>Yas</th>
      <th>Memleket</th>
      <th>Değerlendirme Türü</th>
      <th>Dusunceleriniz</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.Ad %></td>
        <td><%= post.Soyad %></td>
        <td><%= post.Yas %></td>
        <td><%= post.Memleket %></td>
        <td><%= post.category.name %></td>
        <td><%= post.Dusunceleriniz %></td>
        <td><%= link_to 'Detay', post %></td>
        <td><%= link_to 'Güncelle', edit_post_path(post) %></td>
        <td><%= link_to 'Sil', post, method: :delete, data: { confirm: 'Devam Etmek İstiyormusunuz?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<button type="button" class="btn">
<%= link_to 'Yeni Mesaj Giriniz', new_post_path %>
</button>
</div>
</div>
</div>





